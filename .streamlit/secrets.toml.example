# Streamlit secrets configuration
# In Streamlit Cloud, add these in the app settings > Secrets section
# For local development, copy this to .streamlit/secrets.toml

[whoop]
client_id = "your_whoop_client_id"
client_secret = "your_whoop_client_secret"
redirect_uri = "https://your-app-name.streamlit.app"  # Update with your actual Streamlit Cloud URL

[app]
secret_key = "your_random_secret_key_here"
data_start_date = "2026-01-01"
data_end_date = "2026-02-10"

[events]
cardiac_arrest_date = "2026-01-11"
triple_bypass_date = "2026-01-19"
